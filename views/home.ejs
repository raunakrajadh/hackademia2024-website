                                const target = question.getAttribute('data-target');
                                const answer = document.getElementById(target);
                        
                                // Toggle the visibility of the answer
                                answer.style.display = answer.style.display === 'block' ? 'none' : 'block';
                        
                                // Update the icon based on the answer's visibility
                                const icon = question.querySelector('.plus-icon, .minus-icon');
                                if (icon.classList.contains('plus-icon')) {
                                  icon.classList.remove('plus-icon');
                                  icon.classList.add('minus-icon');
                                } else {
                                  icon.classList.remove('minus-icon');
                                  icon.classList.add('plus-icon');
                                }
                              });
                            });
